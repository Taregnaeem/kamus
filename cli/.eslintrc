{
    "env": {
        "browser": false,
        "node": true
    },
    "extends": ["eslint:recommended", "plugin:security/recommended", "prettier"],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": ["security"],
    "rules": {
        "linebreak-style": ["off", "windows"],
        "no-console": ["off"]
    },
    "globals": {
        "it": true,
        "after": true,
        "afterEach": true,
        "before": true,
        "beforeEach": true,
        "describe": true
    }
}
